#include "function_pointers.h"

/**
 * int_index - function that prints a name
 *
 * @array: the element
 * @size: of the array
 * @cmp: the function that compares values
 */

int int_index(int *array, int size, int (*cmp)(int))
{
	int i;

	for (i = 0; i < size; i++)
	{
		if (cmp(array[i]) != 0)
		{
			return (i);
		}
	}
	return (-1);
}
