#include "main.h"

/**
 * times_table - Checks for alphabetic character
 *
 * @c: The character to be checked
 *
 * Return: 1 for alphabetic character or 0 otherwise
 */

void times_table(void)
{
	int n, f;

	n = 0;

	while (n < 9)
	{
		f = 0;
		while (f < 9)
		{
			_putchar(n * f);
				f++;
		}
		_putchar('\n');
		n++;
	}
}
