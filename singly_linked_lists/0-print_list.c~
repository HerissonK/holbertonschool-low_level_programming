#include "lists.h"
#include <stddef.h>
#include <stdio.h>

/**
 * print_list - print all the element from a list "list_t"
 *
 * @h: current node
 *
 * return: the number of nodes
 */

size_t print_list(const list_t *h)
{
	size_t count = 0;

	while (h != NULL)
	{
		if (h->str != NULL)
			printf("%s\n", h-> str);
		else
			printf("[0] (nil)");

		h = h->next;
		count++;
	}
	return (count);
}
