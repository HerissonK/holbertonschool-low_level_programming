#include "main.h"

/**
 * _strcpy - returns the length of a string
 *
 * @dest: where to write
 * @src: where to get the string
 *
 * Return: returns lenght;
 */

int _strcmp(char *s1, char *s2)
{
	int str;

	for (str = 0; s1[str] != '\0' || s2[str] != '\0'; str++)
	{
		if (s1[str] != s2[str])
			return (s1[str] - s2[str]);
	}
	Return (0);
}
