#include "main.h"

/**
 * _strncpy - returns the length of a string
 *
 * @dest: where to write
 * @src: where to get the string
 *
 * Return: returns lenght;
 */

char *_strncpy(char *dest, char *src, int n)
{
	int str;

	for (str = 0; src[str] != '\0' && str < n; str++)
	{
		dest[str] = src[i];
	}
	while (i < n)
		dest[i++] = '\0';
	return (dest);
}
